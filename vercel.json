{
  "buildCommand": "cd BlogPropiedades && npm run build",
  "outputDirectory": "BlogPropiedades/dist",
  "installCommand": "cd BlogPropiedades && npm install",
  "routes": [
    {
      "src": "/api/user/login",
      "dest": "/api/user/login"
    },
    {
      "src": "/api/user/register",
      "dest": "/api/user/register"
    },
    {
      "src": "/api/user/me",
      "dest": "/api/user/me"
    },
    {
      "src": "/api/blogs",
      "dest": "/api/blogs"
    },
    {
      "src": "/api/properties",
      "dest": "/api/properties"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy"
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(js|mjs|jsx|ts|tsx|css|json|ico|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot|otf))",
      "headers": {
        "cache-control": "max-age=31536000, immutable",
        "content-type": "$1"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
